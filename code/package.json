{
  "name": "dash-frontend",
  "version": "1.0.0",
  "description": "Dashboard Frontend",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw ./configs/webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "testMocha": "mocha -t 2000000 --compilers js:babel-register --recursive --reporter spec tools/testSetup.js testMocha/index.js ",
    "testChai": "mocha -t 2000000 --compilers js:babel-register --recursive --reporter spec tools/testSetup.js testChai/*.js ",
    "testEnzyme": "mocha -t 2000000 --compilers js:babel-register --recursive --reporter spec tools/testSetup.js testEnzyme/*.js ",
    "test": "mocha -t 2000000 --compilers js:babel-register --recursive --reporter spec tools/testSetup.js test/components/ComposeComponent/*.spec.js",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -t 2000000 --compilers js:babel-register --recursive tools/testSetup.js test/components/common/*.spec.js",
    "coverage": "istanbul check-coverage"
  },
  "keywords": [],
  "author": "WEB Service",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "eslint": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy": "^1.16.2",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.4",
    "loader-utils": "^1.1.0",
    "mocha": "^4.0.1",
    "mocha-generators": "^2.0.0",
    "mochawesome": "^2.3.1",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "open": "^0.0.5",
    "postcss": "^6.0.12",
    "postcss-center": "^1.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.3",
    "sinon": "^4.0.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-datepicker": "^0.59.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
