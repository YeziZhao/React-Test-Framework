{
  "name": "dash-frontend",
  "version": "1.0.0",
  "description": "Dashboard Frontend",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw ./configs/webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "testMocha": "mocha -t 2000000 --compilers js:babel-register --recursive --reporter mochawesome tools/testSetup.js testMocha/index.js ",
    "testChain": "mocha -t 2000000 --compilers js:babel-register --recursive --reporter mochawesome tools/testSetup.js testChain/*.js ",
    "testEnzyme": "mocha -t 2000000 --compilers js:babel-register --recursive --reporter mochawesome tools/testSetup.js testEnzyme/*.js ",
    "test:watch": "npm run testEnzyme -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "https://oss.navercorp.com/wm-dashboard/dash-frontend.git"
  },
  "keywords": [],
  "author": "WEB Service",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.2",
    "eslint": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy": "^1.16.2",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.4",
    "loader-utils": "^1.1.0",
    "mocha": "^3.5.3",
    "mocha-generators": "^2.0.0",
    "mochawesome": "^2.3.1",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "open": "^0.0.5",
    "postcss": "^6.0.12",
    "postcss-center": "^1.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^3.0.1",
    "sass-loader": "^6.0.3",
    "sinon": "^4.0.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "qs": "^6.5.1",
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "react": "^15.5.4",
    "react-datepicker": "^0.46.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
